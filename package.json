{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p dev:app dev:api",
    "dev": "npm-run-all -p dev:*",
    "dev:app": "react-scripts start",
    "dev:storybook": "start-storybook -p 9009 -s public",
    "dev:api": "json-server db.json --port 4000",
    "test": "react-scripts test --testMatch=**/__tests__/**/*.test.{js,jsx}",
    "test:coverage": "yarn test && cat ./coverage/lcov.info | coveralls",
    "build:app": "react-scripts build",
    "build:docs": "npm-run-all -p build:docs:*",
    "build:docs:storybook": "build-storybook -c .storybook -o docs/reference"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/!(*.test).{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/__tests__/**/*.test.{js,jsx}": [
      "eslint --fix",
      "yarn test --bail --findRelatedTests --watchAll=false",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/src/__tests__/**",
      "!<rootDir>/src/**/*.stories.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/travis-cli": "^8.1.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-centered": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/react": "^8.0.4",
    "@types/jest": "^24.0.15",
    "axios-mock-adapter": "^1.17.0",
    "coveralls": "^3.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "history": "^4.9.0",
    "husky": "^3.0.0",
    "jest-dom": "^3.5.0",
    "jest-extended": "^0.11.2",
    "json-server": "^0.15.0",
    "lint-staged": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "redux-mock-store": "^1.5.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@rocketseat/unform": "^1.5.1",
    "axios": "^0.19.0",
    "immer": "^3.1.3",
    "lodash": "^4.17.14",
    "prop-types": "^15.7.2",
    "react": "16.9.0-alpha.0",
    "react-dom": "16.9.0-alpha.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-router-modal-gallery": "^1.1.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "styled-components": "^4.3.2",
    "unform-material-ui": "^0.2.0",
    "yup": "^0.27.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
